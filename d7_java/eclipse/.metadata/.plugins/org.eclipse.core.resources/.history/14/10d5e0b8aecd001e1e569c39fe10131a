//import java.util.Arrays;
import java.util.Scanner;

public class praticando {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int angle1, angle2;
		
		System.out.println("Digite dois valores inteiros: ");
		angle1 = sc.nextInt();
		angle2 = sc.nextInt();
		
		System.out.println(otherAngle(angle1, angle2));
		
		sc.close();
	}
	
/*
Você recebe dois(null) ângulos internos (em graus) de um triângulo.

Escreva uma função para retornar o terceiro.

Nota: apenas números inteiros positivos serão testados.
*/
	
	public static int ThirdAngle(int angle1, int angle2) {
		int angle3;
		
		angle3 = 180 -(angle1 + angle2);
		
		return angle3;
	}
	
	
//	### COMPARANDO PARÂMETROS DE UMA FUNÇÃO ###
	static String greet(String name, String ownser) {
		if (name == ownser) {
			return "Olá chefe";
		} else {
			return "Olá convidado";
		}
	}
	
//	### TROCANDO OS VALORES DE UMA STRING POR 1 SE MAIOR QUE 5, E POR 0 SE MENOR QUE 5 ###
	public static String fakeBin(String numberString) {
//		repartindo string
		String[] arr = numberString.split("");
		String aux = "";
		
//		percorrendo o array


for (int i = 0; i < arr.length; i++) {
			if(Integer.parseInt(arr[i]) < 5) {
				arr[i] = "0";
			} else if (Integer.parseInt(arr[i]) >= 5) arr[i] = "1";
			
			aux += arr[i];
		}
		
		return aux;
	}

}
